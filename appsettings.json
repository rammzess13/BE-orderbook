{
  "ConnectionStrings": {
    "DefaultConnection": "Data Source=OrderBook.db"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.EntityFrameworkCore": "Warning",
      "OrderBook.Services": "Warning"
    },
    "Console": {
      "FormatterName": "Simple",
      "FormatterOptions": {
        "SingleLine": true,
        "IncludeScopes": false,
        "TimestampFormat": "HH:mm:ss "
      }
    }
  },
  "AllowedHosts": "*",
  "ExternalApi": {
    "WebSocketUrl": "wss://ws.bitstamp.net"
  }
}
